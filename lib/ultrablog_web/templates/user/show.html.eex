<h2>User Info</h2>

<ul>

  <li>
    <strong>Email:</strong>
    <%= @user.email %>
  </li>

  <li>
    <strong>Name:</strong>
    <%= @user.name %>
  </li>

</ul>

<%= if @current_user do %>
  <span><%= link "Back to main", to: "../main" %></span>
<% else %>
  <span><%= link "Back to Log in", to: page_path(@conn, :index) %></span>
<% end %>
